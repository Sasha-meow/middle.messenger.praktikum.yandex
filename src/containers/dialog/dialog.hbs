<div class="w-100 {{ styles.dialog-wrapper }}">
    {{> dialogHeader headerProps }}
    <div class="{{ styles.dialog-wrapper__message-box }}">
        {{#if messages}}
            {{> legend propsLegend}}
            {{#each messages}}
                {{> message propsMessage }}
            {{/each}}
        {{/if}}
    </div>
    {{> dialogField fieldProps }}
</div>
